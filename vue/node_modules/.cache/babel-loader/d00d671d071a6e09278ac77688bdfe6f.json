 {\n          _this.popIndex = message[0]; //设置弹窗的类型\n\n          if (message[0] == 'toast') {\n            _this.toastbox = 'tshow';\n            _this.toast = message[2];\n            setTimeout(function () {\n              _this.toastbox = '';\n              _this.toast = message[2];\n            }, 1500);\n          } else {\n            _this.popslide = ''; //显示popbox弹窗...\n            //对不同的参数进行分类，显示不同的弹窗\n\n            if (message[0] == 'imgcode') {\n              setTimeout(function () {\n                _this.scale = '';\n              }, 200);\n            } else {\n              if (message[0] == 1) {\n                _this.pop_tips = '';\n              } else if (message[0] == 2) {\n                _this.pop_btm = '';\n              }\n\n              setTimeout(function () {\n                _this.scale = '';\n                _this.btext = message[2]; //2设置消息主体\n              }, 200);\n\n              if (message[1] == 'close') {\n                setTimeout(function () {\n                  _this.closePop();\n                }, 2000);\n              }\n            }\n          }\n        }\n      });\n    },\n    closePop: function closePop() {\n      var _this2 = this;\n\n      this.scale = 'scale';\n      setTimeout(function () {\n        _this2.popslide = 'hide';\n        _this2.pop_tips = 'hide';\n      }, 200);\n    }\n  }\n};",{"version":3,"sources":["pop.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;AAuBA,OAAA,MAAA,MAAA,8BAAA;AACA,eAAA;AACA,EAAA,IAAA,EAAA,WADA;AAEA,EAAA,IAAA,EAAA,gBAAA;AACA,WAAA;AACA,MAAA,QAAA,EAAA,MADA;AAEA,MAAA,KAAA,EAAA,OAFA;AAGA,MAAA,KAAA,EAAA,EAHA;AAGA;AACA,MAAA,QAAA,EAAA,MAJA;AAIA;AACA,MAAA,OAAA,EAAA,MALA;AAKA;AACA,MAAA,QAAA,EAAA,MANA;AAMA;AACA,MAAA,QAAA,EAAA,CAPA;AAQA,MAAA,KAAA,EAAA,EARA;AAQA;AACA,MAAA,QAAA,EAAA,EATA;AASA;AACA,MAAA,QAAA,EAAA,EAVA,CAUA;;AAVA,KAAA;AAYA,GAfA;AAgBA,EAAA,OAAA,EAAA,mBAAA;AACA,SAAA,OAAA;AACA,GAlBA;AAmBA,EAAA,OAAA,EAAA;AACA;AACA;AACA;AACA;AAEA,IAAA,OAAA,EAAA,mBAAA;AAAA;;AACA,MAAA,MAAA,CAAA,GAAA,CAAA,QAAA,EAAA,UAAA,OAAA,EAAA;AAAA;AACA,YAAA,OAAA,CAAA,MAAA,EAAA;AACA,UAAA,KAAA,CAAA,QAAA,GAAA,OAAA,CAAA,CAAA,CAAA,CADA,CACA;;AACA,cAAA,OAAA,CAAA,CAAA,CAAA,IAAA,OAAA,EAAA;AACA,YAAA,KAAA,CAAA,QAAA,GAAA,OAAA;AACA,YAAA,KAAA,CAAA,KAAA,GAAA,OAAA,CAAA,CAAA,CAAA;AACA,YAAA,UAAA,CAAA,YAAA;AACA,cAAA,KAAA,CAAA,QAAA,GAAA,EAAA;AACA,cAAA,KAAA,CAAA,KAAA,GAAA,OAAA,CAAA,CAAA,CAAA;AACA,aAHA,EAGA,IAHA,CAAA;AAIA,WAPA,MAOA;AACA,YAAA,KAAA,CAAA,QAAA,GAAA,EAAA,CADA,CACA;AACA;;AACA,gBAAA,OAAA,CAAA,CAAA,CAAA,IAAA,SAAA,EAAA;AACA,cAAA,UAAA,CAAA,YAAA;AACA,gBAAA,KAAA,CAAA,KAAA,GAAA,EAAA;AACA,eAFA,EAEA,GAFA,CAAA;AAGA,aAJA,MAIA;AACA,kBAAA,OAAA,CAAA,CAAA,CAAA,IAAA,CAAA,EAAA;AACA,gBAAA,KAAA,CAAA,QAAA,GAAA,EAAA;AACA,eAFA,MAEA,IAAA,OAAA,CAAA,CAAA,CAAA,IAAA,CAAA,EAAA;AACA,gBAAA,KAAA,CAAA,OAAA,GAAA,EAAA;AACA;;AACA,cAAA,UAAA,CAAA,YAAA;AACA,gBAAA,KAAA,CAAA,KAAA,GAAA,EAAA;AACA,gBAAA,KAAA,CAAA,KAAA,GAAA,OAAA,CAAA,CAAA,CAAA,CAFA,CAEA;AACA,eAHA,EAGA,GAHA,CAAA;;AAIA,kBAAA,OAAA,CAAA,CAAA,CAAA,IAAA,OAAA,EAAA;AACA,gBAAA,UAAA,CAAA,YAAA;AACA,kBAAA,KAAA,CAAA,QAAA;AACA,iBAFA,EAEA,IAFA,CAAA;AAGA;AACA;AACA;AAEA;AACA,OApCA;AAqCA,KA5CA;AA6CA,IAAA,QA7CA,sBA6CA;AAAA;;AACA,WAAA,KAAA,GAAA,OAAA;AACA,MAAA,UAAA,CAAA,YAAA;AACA,QAAA,MAAA,CAAA,QAAA,GAAA,MAAA;AACA,QAAA,MAAA,CAAA,QAAA,GAAA,MAAA;AACA,OAHA,EAGA,GAHA,CAAA;AAIA;AAnDA;AAnBA,CAAA","sourcesContent":["<template>\r\n    <div class=\"pop_conta\">\r\n        <div class=\"popbox\" v-bind:class=\"popslide\" @click=\"closePop\">\r\n            <div class=\"pop_box\" v-bind:class=\"[scale,pop_tips]\">   <!--message[0]==1-->\r\n                <div class=\"silgle_text\">{{btext}}</div>\r\n            </div>\r\n            <div class=\"pop_box\" v-bind:class=\"[scale,pop_btm]\">    <!--message[0]==2-->\r\n                <div class=\"silgle_text\">{{btext}}</div>\r\n                <div class=\"cdd\">222222</div>\r\n            </div>\r\n            <div class=\"pop_box\" v-bind:class=\"[scale,pop_code]\">\r\n                <div class=\"box_tips\">输入下方二维码</div>\r\n                <div class=\"code_box clearfix\">\r\n                    <div class=\"in_box left\"><input type=\"number\" id=\"imgcode\" name=\"imgcode\" value=\"\" /></div>\r\n                    <div class=\"img right\"><img :src=\"code_img\"/></div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n        <div class=\"toast\" v-bind:class=\"toastbox\">{{toast}}</div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import popbox from '../../assets/script/popup.js'\r\n    export default{\r\n        name:'pop_conta',\r\n        data:()=>{\r\n            return{\r\n                popslide:'hide',\r\n                scale:'scale',\r\n                btext:'',           //实体字\r\n                pop_tips:'hide',    //控制第一个弹窗\n                pop_btm:'hide',     //控制第二个弹窗\r\n                pop_code:'hide',    //控制二维码弹窗\r\n                popIndex:1,\r\n                toast:'',           //toast显示的内容\r\n                toastbox:'',        //控制toast显示\r\n                code_img:''          //二维码图片地址\r\n            }\r\n        },\r\n        created:function(){\r\n           this.showpop();\r\n        },\r\n        methods:{\r\n            //弹窗参数说明：--------------------------------------------------------------------\r\n            // message[0]为弹窗的类型，按照pop_box的顺序。当message[0]为tshow时底部提示\r\n            //message[1]为close时 为弹窗是否可以自动关闭\r\n            //message[2]为弹窗主要提示的信息\r\n            \r\n            showpop:function(){\r\n                popbox.$on(\"popFun\",(message)=>{   //这里最好用箭头函数，不然this指向有问题\r\n                    if(message.length){\r\n                        this.popIndex = message[0];//设置弹窗的类型\r\n                        if(message[0] == 'toast'){\r\n                            this.toastbox='tshow';\r\n                            this.toast=message[2];\r\n                            setTimeout(()=>{\r\n                                this.toastbox='';\r\n                                this.toast=message[2];\r\n                            },1500);\r\n                        }else{\r\n                            this.popslide=''; //显示popbox弹窗...\r\n                            //对不同的参数进行分类，显示不同的弹窗\r\n                            if(message[0] == 'imgcode'){\r\n                                setTimeout(()=>{\r\n                                    this.scale='';\r\n                                },200);\r\n                            }else{\r\n                                if(message[0] == 1){\r\n                                    this.pop_tips='';\r\n                                }else if(message[0] == 2){\r\n                                    this.pop_btm='';\n                                }\r\n                                setTimeout(()=>{\r\n                                    this.scale='';\r\n                                    this.btext = message[2]; //2设置消息主体\r\n                                },200);\r\n                                if(message[1] == 'close'){\r\n                                    setTimeout(()=>{\r\n                                        this.closePop();\r\n                                    },2000);\r\n                                }\r\n                            }\r\n                        }\r\n                        \r\n                    }\r\n                })\r\n            },\r\n            closePop(){\r\n                this.scale='scale';\r\n                setTimeout(()=>{\r\n                    this.popslide='hide';\r\n                    this.pop_tips='hide';\r\n                },200)\r\n            }\r\n        }\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n    .toast{position: fixed;bottom: 0rem;width: 40%;left: 30%;text-align: center;border-radius: 0.2rem;font-size: 0.16rem;padding: 0.05rem 0;color:#fff;background:#D85A57;-webkit-transition: all .2s;transition:all .2s;transform: translateY(100%);-webkit-transform: translateY(100%);opacity: 0.8;}\r\n    .toast.tshow{transform: translateY(-200%);-webkit-transform: translateY(-200%);}\r\n    \r\n    .popbox{position: fixed;position: fixed;top: 0;left: 0;width: 100%;height: 100%;background: rgba(0,0,0,0.4);}\r\n    .pop_box{position: fixed;width: 80%;left: 10%;top: 30%;background: #fff;text-align: center;border-radius: 0.1rem;-webkit-transition: all .3s;transition:all .3s;}\r\n    .pop_box.scale{transform: scale(0);}\r\n    .pop_box .silgle_text{line-height: 0.8rem;}\r\n</style>"],"sourceRoot":"src\\components\\popup"}]}